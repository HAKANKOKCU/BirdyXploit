<Window x:Class="BirdyXploit.Settings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BirdyXploit"
        mc:Ignorable="d"
        Title="Settings" Height="400" Width="300">
    <StackPanel>
        <CheckBox Name="Darkmode" Click="Darkmode_Click">Dark Mode</CheckBox>
        <CheckBox Name="Topmosta" Click="Topmost_Click">Top Most</CheckBox>
        <CheckBox Name="AutoInject" Click="AutoInject_Click">Auto Inject</CheckBox>
        <Button BorderThickness="0" Style="{StaticResource OrangeButton}" Name="RestartButton" Click="RestartButton_Click">Restart</Button>
    </StackPanel>
</Window>
